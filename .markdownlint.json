{
  "default": true,
  "header-increment": false,
  "header-style": {
    "style": "atx"
  },
  "ul-style": {
    "style": "asterisk"
  },
  "ul-indent": {
    "indent": 2
  },
  "line-length": false,
  "no-duplicate-header": {
    "siblings_only": true
  },
  "ol-prefix": {
    "style": "ordered"
  },
  "no-inline-html": false,
  "no-bare-urls": false,
  "no-multiple-blanks": false,
  "hr-style": {
    "style": "---"
  },
  "fenced-code-language": false,
  "emphasis-style": {
    "style": "underscore"
  },
  "strong-style": {
    "style": "asterisk"
  },
  "link-fragments": true,
  "reference-links-images": true,
  "link-image-reference-definitions": true
}
